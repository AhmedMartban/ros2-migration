<?xml version='1.0' encoding='utf-8'?>
<package format="3">
  <name>rviz_path_array</name>
  <version>0.1.0</version>
  <description>Path array display plugin for rviz</description>
  <author email="ptsingaman@laas.fr">Phani Teja Singamaneni</author>
  <maintainer email="ptsingaman@laas.fr">Phani Teja Singamaneni</maintainer>
  <license>BSD</license>
  <buildtool_depend>ament_cmake</buildtool_depend>
  <build_depend>qtbase5-dev</build_depend>
  <build_depend>rviz</build_depend>
  <build_depend>cohan_msgs</build_depend>
  <exec_depend>libqt5-core</exec_depend>
  <exec_depend>libqt5-gui</exec_depend>
  <exec_depend>libqt5-widgets</exec_depend>
  <exec_depend>rviz</exec_depend>
  <exec_depend>cohan_msgs</exec_depend>
  <export>
      <rviz plugin="${prefix}/plugin_description.xml" />
